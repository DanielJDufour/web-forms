{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputMode": "new-only"
    },
    "suid#build": {
      "outputMode": "new-only"
    },
    "@odk/xpath#build": {
      "dependsOn": ["tree-sitter-xpath#build"],
      "outputMode": "new-only"
    },
    "@odk/web-forms#build": {
      "dependsOn": ["suid#build", "@odk/xpath#build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "outputs": []
    }
  }
}
