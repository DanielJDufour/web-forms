<?xml version="1.0"?><h:html xmlns="http://www.w3.org/2002/xforms" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:odk="http://www.opendatakit.org/xforms"><h:head><h:title>Disaster Incidence</h:title><model odk:xforms-version="1.0.0"><instance><data id="Disaster-Incidence" version="20240917"><event_name/><incident_type/><incident_others/><location/><primary_photo/><photogroup jr:template=""><photo/></photogroup><photogroup><photo/></photogroup><remarks/><device_id/><start_time/><end_time/><username/><email/><meta><instanceID/></meta></data></instance><instance id="incident_type"><root><item><name>flood</name><label>Flood</label></item><item><name>landslide</name><label>Landslide</label></item><item><name>earthquake</name><label>Earthquake</label></item><item><name>damages</name><label>Damages</label></item><item><name>volcanic_activity</name><label>Volcanic Activity</label></item><item><name>others</name><label>Others</label></item></root></instance><bind nodeset="/data/event_name" type="string" required="true()"/><bind nodeset="/data/incident_type" type="string" required="true()"/><bind nodeset="/data/incident_others" type="string" relevant=" /data/incident_type  = 'other'"/><bind nodeset="/data/location" type="geopoint" required="true()"/><bind nodeset="/data/primary_photo" type="binary" required="true()" orx:max-pixels="1024"/><bind nodeset="/data/photogroup/photo" type="binary" required="true()" orx:max-pixels="1024"/><bind nodeset="/data/remarks" type="string"/><bind nodeset="/data/device_id" jr:preload="property" type="string" jr:preloadParams="deviceid"/><bind nodeset="/data/start_time" jr:preload="timestamp" type="dateTime" jr:preloadParams="start"/><bind nodeset="/data/end_time" jr:preload="timestamp" type="dateTime" jr:preloadParams="end"/><bind nodeset="/data/username" jr:preload="property" type="string" jr:preloadParams="username"/><bind nodeset="/data/email" jr:preload="property" type="string" jr:preloadParams="email"/><bind nodeset="/data/meta/instanceID" type="string" readonly="true()" jr:preload="uid"/></model></h:head><h:body><input ref="/data/event_name"><label>Event Name</label><hint>(Typhoon name, Disaster location, etc.)</hint></input><select1 ref="/data/incident_type"><label>Type of Incident</label><hint>Select the incident classification of the event.</hint><itemset nodeset="instance('incident_type')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><input ref="/data/incident_others"><label>Others</label><hint>Type of Incident</hint></input><input ref="/data/location"><label>Location</label><hint>Click start GeoPoint and wait for your device to detect your gps location.</hint></input><upload mediatype="image/*" ref="/data/primary_photo"><label>Photo</label><hint>Take a picture of the incident.</hint></upload><group ref="/data/photogroup"><label>Add another photo.</label><repeat nodeset="/data/photogroup"><upload mediatype="image/*" ref="/data/photogroup/photo"><label>Photo</label><hint>Take a picture of the incident.</hint></upload></repeat></group><input ref="/data/remarks"><label>Remarks</label><hint>Additional information about the incident.</hint></input></h:body></h:html>