{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputMode": "new-only"
    },
    "@getodk/tree-sitter-xpath#build": {
      "outputMode": "new-only"
    },
    "@getodk/xpath#build": {
      "dependsOn": ["@getodk/tree-sitter-xpath#build"],
      "outputMode": "new-only"
    },
    "@getodk/xforms-engine#build": {
      "dependsOn": ["@getodk/xpath#build"],
      "outputs": ["dist/**"]
    },
    "@getodk/ui-vue#build": {
      "dependsOn": ["@getodk/xforms-engine#build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "outputs": []
    }
  }
}
